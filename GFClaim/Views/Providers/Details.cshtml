@model GFClaim.Models.Provider
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name </h2>
<ul>
    <li>Address : @Model.Address1 , @Model.Address2, @Model.City, @Model.State @Model.Zipcode</li>
    <li>Phone : @Model.Phone</li>
    <li>Tax ID : @Model.TaxIdentity </li>
    <li>Type : @Model.ProviderType.TypeName </li>
</ul>
